{
	"tsserver.log": "verbose",
	"tsserver.trace.server": "verbose",
	"elixir.pathToElixirLS": "/opt/elixir-ls/release/language_server.sh",
	"elixirLS.dialyzerEnabled": false,
	"java.format.settings.url": "$HOME/example/path/java-code-formatter-prefs.xml",
	"java.home": "/home/nicolas/.sdkman/candidates/java/11.0.8.hs-adpt/",
	"java.trace.server": "verbose",
	"java.jdt.ls.vmargs": "-noverify -Xmx6G -XX:+UseG1GC -XX:+UseStringDeduplication",
	"java.completion.importOrder": [
		"java",
		"javax",
		"org",
		"com"
	],
	"java.configuration.runtimes": [
		{
			"name": "JavaSE-1.8",
			"path": "/home/nicolas/.sdkman/candidates/java/8.0.265.j9-adpt/"
		},
		{
			"name": "JavaSE-11",
			"path": "/home/nicolas/.sdkman/candidates/java/11.0.8.hs-adpt/",
			"default": true
		}
	],
	"coc.preferences.formatOnSaveFiletypes": [
		"sbt",
		"xml",
		"scala",
		"ocaml",
		"java",
		"json",
		"typescript",
		"typescriptreact",
		"javascript",
		"javascriptreact",
		"html",
		"css",
		"scss",
		"markdown"
	],
	"languageserver": {
		"erlang": {
			"command": "erlang_ls",
			"filetypes": [
				"erlang"
			]
		},
		"racket": {
			"command": "racket",
			"args": [
				"--lib racket-langserver"
			],
			"filetypes": [
				"racket",
				"scheme"
			],
			"trace.server": "verbose"
		},
		"haskell": {
			"command": "haskell-language-server-wrapper",
			"args": [
				"--lsp"
			],
			"rootPatterns": [
				"*.cabal",
				"stack.yaml",
				"cabal.project",
				"package.yaml",
				"hie.yaml"
			],
			"filetypes": [
				"haskell",
				"lhaskell"
			]
		},
		"ocaml-lsp": {
			"command": "opam",
			"rootPatterns": [
				"dune-project"
			],
			"args": [
				"config",
				"exec",
				"--",
				"ocamllsp"
			],
			"filetypes": [
				"ocaml",
				"reason"
			]
		},
		"elixirLS": {
			"command": "/opt/elixir-ls/release/language_server.sh",
			"filetypes": [
				"elixir",
				"eelixir"
			]
		},
		"clojure-lsp": {
			"command": "bash",
			"args": [
				"-c",
				"clojure-lsp"
			],
			"filetypes": [
				"text",
				"clojure"
			],
			"rootPatterns": [
				"project.clj"
			],
			"additionalSchemes": [
				"jar",
				"zipfile"
			],
			"trace.server": "verbose",
			"initializationOptions": {}
		}
	}
}
